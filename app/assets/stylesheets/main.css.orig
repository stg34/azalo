/******************************************************************************/
/* North-West corner */
.azalo_nw {
  background: transparent url(azalo/left-top.gif) no-repeat 0 0;
  width:10px;
  height:25px;
}

/* North border */
.azalo_n {
  background: transparent url(azalo/top-middle.gif) repeat-x 0 0;
  /*height:25px;*/
}

/* North-East corner */
.azalo_ne {
  background: transparent url(azalo/right-top.gif) no-repeat 0 0;
  width:10px;
  height:25px;
}

/* West border */
.azalo_w {
  background: transparent url(azalo/frame-left.gif) repeat-y top left;
  width:7px;
}

/* East border */
.azalo_e {
  background: transparent url(azalo/frame-right.gif) repeat-y top right;
  width:7px;
}

/* South-West corner */
.azalo_sw {
  background: transparent url(azalo/bottom-left-c.gif) no-repeat 0 0;
  width:7px;
  height:7px;
}

/* South border */
.azalo_s {
  background: transparent url(azalo/bottom-middle.gif) repeat-x 0 0;
  height:7px;
}

/* South-East corner */
.azalo_se {
  background: transparent url(azalo/bottom-right-c.gif) no-repeat 0 0;
  width:7px;
  height:7px;
  vertical-align:top;
}

/* Resize button */
.azalo_sizer {
  width:7px;
  height:7px;
    background: transparent url(azalo/RESIZE_IMAGE.gif) no-repeat 0 0;
    cursor:se-resize;
}

/* Close button */
.azalo_close {
    width: 23px;
    height: 23px;
    background: transparent url(azalo/button-close-focus.gif) no-repeat 0 0;
    position:absolute;
    top:0px;
    right:11px;
    cursor:pointer;
    z-index:1000;
}

/* Minimize button */
.azalo_minimize {
    width: 23px;
    height: 23px;
    background: transparent url(azalo/button-min-focus.gif) no-repeat 0 0;
    position:absolute;
    top:0px;
    right:55px;
    cursor:pointer;
    z-index:1000;
}

/* Maximize button */
.azalo_maximize {
    width: 23px;
    height: 23px;
    background: transparent url(azalo/button-max-focus.gif) no-repeat 0 0;
    position:absolute;
    top:0px;
    right:33px;
    cursor:pointer;
    z-index:1000;
}

/* Title bar */
.azalo_title {
    float:left;
    height:14px;
    font-size:14px;
    text-align:center;
    margin-top:2px;
    width:100%;
    color:#123456;
}

